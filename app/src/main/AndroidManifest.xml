<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="softupcloud.appdistribuidas">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PlaceActivity" />
        <activity android:name=".SwActivity" />
        <activity android:name=".CoSortActivity" />
        <activity android:name=".ShActivity" />
        <activity android:name=".DF_BeneficiarioActivity" />
        <activity android:name=".DF_ColaboradorActivity" />
        <activity android:name=".DF_FotoActivity" />
        <activity android:name=".DF_TipoObraActivity"></activity>
    </application>

</manifest>